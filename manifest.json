{
  "name": "koaembed",
  "vendor": "kingofapppartneres",
  "version": "4.1.7",
  "title": "Vtex to mobile app KOA App Builder",
  "description": "Integrate your vtex website into King Of App",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true,
    "type": "free",
    "availableCountries": ["*"]
  },
  "policies": [
    {
      "name": "catalog",
      "effect": "allow"
    },
    {
      "name": "storefront",
      "effect": "allow"
    }
  ],
  "modules": {
    "koa-menu": {
      "route": "node/koa-menu",
      "path": "./node/koa-menu"
    }
  },
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x",
    "node": "6.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x",
    "vtex.menu": "2.x"
  },
  "settingsSchema": {
    "title": "KOA embed",
    "type": "object",
    "properties": {
      "koaEmbedId": {
        "title": "Embed Id",
        "description": "Enter the querystring to enable embed mode",
        "type": "string"
      },
      "koaEmbedStyle": {
        "title": "Embed style",
        "description": "Enter the styles to be added when the page is in embed mode",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
